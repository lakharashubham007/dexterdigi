{
  "extends": ["eslint:recommended", "next", "next/core-web-vitals", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ],
      "no-unused-vars": "off",
      "react-hooks/exhaustive-deps": "off",
    "@next/next/no-img-element": "off"
  }
}
